# Redis (optional for tests)
REDIS_URL=redis://localhost:6379
REDIS_ENABLED=false

# Performance
PERFORMANCE_MONITORING=false
ANALYTICS_ENABLED=true

# Security Headers
HELMET_ENABLED=true
CORS_ENABLED=true
